{
  "name": "plexix-vault-protocol",
  "version": "1.0.0",
  "description": "PlexiX Vault Protocol - Move contracts, integration tests, and production backend on Aptos",
  "main": "backend/dist/index.js",
  "scripts": {
    "dev": "nodemon backend/src/index.ts",
    "build": "tsc -p backend/tsconfig.json",
    "start": "node backend/dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "mocha --require ts-node/register --timeout 30000 tests/integration/**/*.test.ts",
    "test:integration:watch": "npm run test:integration -- --watch",
    "lint": "eslint backend/src/**/*.ts",
    "lint:fix": "eslint backend/src/**/*.ts --fix",
    "format": "prettier --write backend/src/**/*.ts",
    "docker:build": "docker build -t plexix-backend .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "move:compile": "aptos move compile",
    "move:test": "aptos move test",
    "move:publish": "aptos move publish"
  },
  "keywords": [
    "aptos",
    "move",
    "vault",
    "defi",
    "web3",
    "typescript",
    "backend"
  ],
  "author": "PlexiX Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.29.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^4.3.11",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chai": "^4.3.10",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
